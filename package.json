{
  "name": "flyback",
  "version": "3.0.0",
  "main": "index.cjs.js",
  "module": "index.esm.js",
  "types": "typings/index.d.ts",
  "license": "MIT",
  "description": "A node.js HTTP proxy that records and playbacks requests",
  "scripts": {
    "build": "yarn lint && yarn typecheck && yarn build:cleanup && yarn build:rollup && yarn build:typings",
