    "fast-deep-equal": "^2.0.1",
    "iltorb": "^2.4.3",
    "mime-format": "^2.0.0",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.6.0",
    "pluralize": "^8.0.0",
    "query-string": "^6.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
