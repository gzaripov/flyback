    "build:rollup": "rollup -c",
    "build:typings": "tsc --declaration --emitDeclarationOnly --outDir dist/typings",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "start": "node ./example/start.js",
    "test": "jest",
    "jest": "jest",
    "rollup": "rollup",
    "release": "standard-version"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
