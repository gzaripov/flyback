    "fs-extra": "^8.0.1",
    "jest": "^24.8.0",
    "jest-environment-node": "^24.8.0",
    "node-mocks-http": "^1.7.6",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rollup": "^1.14.6",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
